@model TapMiniGames.Models.GameViewModel
@{
    ViewBag.Title = "Buffalo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title>Game</title>
    <link rel="stylesheet" type="text/css" href="~/lib//css//buffalo.css">
    <script src="~/lib/js/script.js"></script>
</head>
<body>
    <div class="bg-gradient-to-t flex justify-center w-full max-w-xl px-4" style="height: 100vh">
        <div class="w-full text-white h-screen font-bold flex flex-col">
            <div class="px-4 z-10">
                <div class="flex items-center space-x-2 pt-4">
                    <div class="text-center text-[#d4d4d4]">
                        <svg width="61" height="62" viewBox="0 0 61 62" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect x="6" y="6" width="50" height="50" rx="10" fill="#FFCC0F" />
                            <rect width="61" height="62" fill="url(#pattern0_23_584)" />
                            <defs>
                                <pattern id="pattern0_23_584" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_23_584" transform="matrix(0.00225865 0 0 0.00222222 -0.00819672 0)" />
                                </pattern>
                                <image id="image0_23_584" width="450" height="450" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </div>
                    <div>
                        <p class="text-sm" style="font-weight: bold; font-size: 16px">Water Buff</p>
                    </div>
                </div>
                <div class="flex items-center justify-between space-x-4 mt-1 border-1 bg-[#3A3629] rounded-full mt-1">
                    <div class="flex items-center w-2/4  p-2">
                        <div class="w-full">
                            <div class="flex justify-between">
                                <p class="text-sm">@Model.LevelNames[Model.LevelIndex] > </p>
                                <p class="text-sm">@Model.LevelIndex <span class="text-[#95908a]">/ @Model.LevelNames.Count</span></p>
                            </div>
                            <div class="flex items-center mt-1">
                                <div class="w-full h-2 bg-[#43433b]/[0.6] rounded-full">
                                    <div class="progress-gradient h-2 rounded-full" style="width: @(Model.CalculateProgress(Model.LevelIndex, Model.Points))%"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="flex items-center w-2/4  rounded-full px-4 py-[2px] max-w-64">
                        @* <div class="h-[32px] w-[2px] bg-[#43433b] mx-2"></div> *@
                        <div class="flex-1 text-center">
                            <p class="text-xs text-[#85827d] font-medium" style="font-size: 13px; color: white; ">INCOME</p>
                            <div class="flex items-center justify-center space-x-1">
                                <img src="~/lib/img/coin.png" alt="Dollar Coin" class="w-[18px] h-[18px]" />
                                <p class="text-sm" style="font-size: 15px; font-weight: bold">@Model.FormatProfitPerHour(Model.ProfitPerHour)</p>
                            </div>
                        </div>
                        @* <div class="h-[32px] w-[2px] bg-[#43433b] mx-2"></div> *@
                        <!-- Settings Icon as SVG -->
                        <svg fill="currentColor"
                             class="w-6 h-6 mx-auto"
                             height="24"
                             width="24"
                             viewBox="0 0 24 24"
                             xmlns="http://www.w3.org/2000/svg">
                            <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                            <g id="SVGRepo_tracerCarrier"
                               stroke-linecap="round"
                               stroke-linejoin="round"></g>
